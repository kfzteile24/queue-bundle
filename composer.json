{
    "name": "kfzteile24/queue-bundle",
    "description": "Provides an abstraction on top of AWS' PHP SDK Sqs- and SnsClient to make it DI friendly.",
    "license": "MIT",
    "type": "symfony-bundle",
    "homepage": "https://github.com/kfzteile24/queue-bundle",
    "keywords": [
        "aws",
        "sqs",
        "sns",
        "queue"
    ],
    "authors": [
        {
            "email": "jacopo.melloni@project-a.com",
            "name": "Jacopo Melloni"
        }
    ],
    "autoload": {
        "psr-4": {
            "Kfz24\\QueueBundle\\": "src/Kfz24"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Kfz24\\QueueBundle\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^7.1",
        "aws/aws-sdk-php": "^3.19",
        "aws/aws-php-sns-message-validator": "~1.1.0",
        "ext-json": "^1.6",
        "ramsey/uuid": "^3.8"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.7.5",
        "symfony/config": "~3.2.0",
        "symfony/dependency-injection": "~3.2.0",
        "symfony/http-kernel": "~3.2.0"
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
